<!DOCTYPE html>
<html>
<head>
  <title>Espnsports</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if notice %>
  <div class="container">
    <div class="alert alert-warning"><%= notice %></div>
  </div>
<% end %>

<div class="container">
  <%= yield %>
</div>

</body>
</html>
